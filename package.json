{
  "name": "web_app_generator",
  "version": "1.1.0",
  "description": "Op to scaffold Apps using popular frameworks.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/index.ts",
    "start": "node lib/index.js",
    "test": "NODE_ENV=test SDK_SPEAK_PORT=12345 mocha ",
    "clean": "rm -rf node_modules",
    "clean-install": "npm run clean && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@cto.ai/sdk": "^2.0.1",
    "@octokit/rest": "^16.38.1",
    "@types/mocha": "^7.0.1",
    "chai": "^4.2.0",
    "mocha": "^7.0.1",
    "simple-git": "^1.131.0",
    "sinon": "^8.1.1",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@octokit/types": "^2.1.1",
    "@types/node": "^12.0.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/index.ts",
      "coverage/",
      "**/*.test.ts"
    ],
    "reporter": [
      "html"
    ]
  }
}
